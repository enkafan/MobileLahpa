@model System.Collections.Generic.List<string>

<nav class="navbar navbar-default" role="navigation">
    <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("LAHPA Mobile", "Index", "Home",null, new { @class = "navbar-brand" })
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="navbar-collapse">
            <ul class="nav navbar-nav">
                @foreach (var league in Model)
                {
                    <li>@Html.ActionLink(league.Replace("C League ", "C "), "Index", "Schedule", new { id = league }, null)</li>
                }
            </ul>
        </div><!-- /.navbar-collapse -->
    </div>
</nav>